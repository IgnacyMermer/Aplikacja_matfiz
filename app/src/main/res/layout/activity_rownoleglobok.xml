                        android:id="@+id/jednostkaKatARownoleglobok"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="750dp"
                        android:hint="Kąt rozwarty"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/katBRownoleglobok"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/katBRownoleglobok"
                        android:text="@string/stopnie"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="745dp"
                        android:textSize="28dp"
                        android:gravity="left|top"
                        android:id="@+id/jednostkaKatBRownoleglobok"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="830dp"
                        android:hint="Kąt między przekątnymi"
                        android:ems="10"
