                        android:textColor="@android:color/white"
                        android:layout_marginTop="273dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaWysokosc2Rownoleglobok"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="350dp"
                        android:hint="Krótsza przekątna"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/przekatnaARownoleglobok"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/przekatnaARownoleglobok"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="353dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPrzekatnaARownoleglobok"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="430dp"
                        android:hint="Dłuższa przekątna"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/przekatnaBRownoleglobok"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/przekatnaBRownoleglobok"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="433dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPrzekatnaBRownoleglobok"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="510dp"
                        android:hint="Pole"
                        android:inputType="numberDecimal"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/poleRownoleglobok"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
