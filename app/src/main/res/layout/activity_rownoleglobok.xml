                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/katCRownoleglobok"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/katCRownoleglobok"
                        android:text="@string/stopnie"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="825dp"
                        android:textSize="28dp"
                        android:gravity="left|top"
                        android:id="@+id/jednostkaKatCRownoleglobok"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="220dp"
                    android:layout_marginTop="1040dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                    <CheckBox
                        android:id="@+id/checkboxPoleRownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Pole"/>
                    <CheckBox
                        android:id="@+id/checkboxObwodRownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="10dp"
                        android:text="Obwód"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:id="@+id/checkboxBokARownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Kr. bok"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp" />

                    <CheckBox
                        android:id="@+id/checkboxKatARownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Kąt ostry"/>
                    <CheckBox
                        android:id="@+id/checkboxBokBRownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="60dp"
                        android:text="Dłuż. bok"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:id="@+id/checkboxKatBRownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Kąt rozwarty"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxPrzekatnaARownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="110dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Krótsza przekątna" />
                    <CheckBox
                        android:id="@+id/checkboxPrzekatnaBRownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="110dp"
                        android:textColor="@android:color/white"
                        android:layout_marginEnd="10dp"
                        android:textSize="15dp"
                        android:buttonTint="@color/checkbox1"
                        android:text="Dłuższa przekątna"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxWysokoscARownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="160dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Krótsza wysokosc" />
                    <CheckBox
                        android:id="@+id/checkboxWysokoscBRownolegloboku"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="160dp"
                        android:textColor="@android:color/white"
                        android:layout_marginEnd="10dp"
                        android:textSize="15dp"
                        android:buttonTint="@color/checkbox1"
                        android:text="Dłuższa wysokość"
                        android:layout_marginRight="10dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="1305dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">

                    <TextView
                        android:id="@+id/pierwszaLiniaRownoleglobok"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/drugaLiniaRownoleglobok"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
