                    android:layout_height="1135dp"
                    android:layout_marginTop="70dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/textview_rounded_black"
                    android:id="@+id/pierwszyFragmencikRownoramienny">
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="30dp"
                        android:hint="Podstawa"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/podstawaRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaPodstawaRoznoboczny"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="36dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="130dp"
                        android:hint="Bok B"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/ramieB"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaRamieB"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="136dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="230dp"
                        android:hint="Bok C"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/ramieC"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaRamieC"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="236dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="330dp"
                        android:hint="Obwód"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/obwodRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaObwodRoznoboczny"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="336dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="430dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Pole"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/poleRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm2"
                        android:id="@+id/jednostkaPoleRoznoboczny"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="436dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="530dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Wysokość"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/wysokoscRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaWysokoscRoznoboczny"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="536dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="630dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Promień okr. wpis."
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/wpisanyRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRoznoboczny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaWpisanyRoznoboczny"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="636dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="730dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Promień okr. opis."
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/opisanyRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
