                        android:layout_marginTop="928dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="1030dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:textColorHint="@android:color/white"
                        android:hint="Kąt przy wierz. C"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/katCRoznoboczny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@+id/katCRoznoboczny"
                        android:textSize="26dp"
                        android:textColor="@android:color/white"
                        android:text="@string/stopnie"
                        android:gravity="left"
                        android:layout_marginTop="1028dp"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="210dp"
                    android:layout_below="@+id/pierwszyFragmencikRownoramienny"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginTop="45dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:id="@+id/drugiFragmencikRownoramienny">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Podstawa"
                        android:layout_marginTop="60dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:textColor="@android:color/white"
                        android:id="@+id/podstawaWyborRoznoboczny"
                        android:textSize="18dp"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bok B"
                        android:layout_marginTop="10dp"
                        android:layout_centerHorizontal="true"
                        android:textSize="18dp"
                        android:layout_marginLeft="4dp"
                        android:textColor="@android:color/white"
                        android:id="@+id/ramieBWyborRoznoboczny"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bok C"
                        android:id="@+id/ramieCWyborRoznoboczny"
                        android:layout_marginTop="60dp"

                        android:layout_centerHorizontal="true"
                        android:buttonTint="@color/checkbox1"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Obwód"
                        android:layout_marginTop="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:textColor="@android:color/white"
                        android:textSize="18dp"
                        android:layout_marginLeft="4dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/obwodWyborRoznoboczny"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pole"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:textSize="16dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/poleWyborRoznoboczny"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Wysokość"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="60dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:textSize="18dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/wysokoscWyborRoznoboczny"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prom. okr. opis."
                        android:layout_marginTop="110dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:textColor="@android:color/white"
                        android:textSize="18dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/opisanyWyborRoznoboczny"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prom. okr. wpis."
                        android:layout_marginTop="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/wpisanyWyborRoznoboczny"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kąt A"
                        android:layout_marginTop="160dp"
                        android:layout_marginLeft="2dp"
                        android:layout_marginStart="10dp"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/katAWyborRoznoboczny"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kąt B"
                        android:layout_marginTop="160dp"
                        android:layout_centerHorizontal="true"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/katBWyborRoznoboczny"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kąt C"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="160dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:textSize="18dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/katCWyborRoznoboczny"
                        android:layout_marginRight="10dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:background="@drawable/textview_rounded_black"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginTop="45dp"
                    android:id="@+id/trzeciFragmencikRownoramienny"
                    android:layout_below="@id/drugiFragmencikRownoramienny"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:id="@+id/pierwszaLiniaRoznoboczny"
                        android:textColor="@android:color/white"
                        android:layout_centerHorizontal="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:layout_marginTop="55dp"
                        android:id="@+id/drugaLiniaRoznoboczny"
                        android:textColor="@android:color/white"
                        android:layout_centerHorizontal="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:layout_marginTop="110dp"
                        android:id="@+id/trzeciaLiniaRoznoboczny"
                        android:textColor="@android:color/white"
                        android:layout_centerHorizontal="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:layout_marginTop="165dp"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/czwartaLiniaRoznoboczny"
                        android:textColor="@android:color/white"
