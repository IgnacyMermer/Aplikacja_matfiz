<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".trapez_prostokatny"
    android:id="@+id/drawerTrapez_prostokatny_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rellayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:id="@+id/rellayoutTopBar"
            android:background="@android:color/black"
            android:layout_alignParentTop="true">
            <com.google.android.material.appbar.AppBarLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.google.android.material.appbar.AppBarLayout>
            <ImageButton
                android:id="@+id/buttonBar"
                android:layout_width="30dp"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:layout_height="30dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_menu_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rellayoutTopBar"
            android:layout_above="@id/rellayoutBottomBar"
            android:background="@drawable/gradient_list"
            android:id="@+id/rellayoutMiddle">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rellayoutTopBar"
                android:layout_above="@+id/rellayoutBottomBar">
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:text="Wyczyść wszystko"
                    android:id="@+id/buttonCzyscProstokatny"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="925dp"
                    android:layout_marginTop="70dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:hint="Dolna podstawa"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/dolnaProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:hint="Górna podstawa"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/gornaProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/dolnaProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="33dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkadolnaProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/gornaProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="113dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaGornaProstokatny"/>

                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="190dp"
                        android:hint="Wysokość"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/wysokoscProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/wysokoscProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="193dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaWysokoscProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="270dp"
                        android:hint="Czwarty bok"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/czwartyBokProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/czwartyBokProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="273dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaCzwartyBokProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="350dp"
                        android:hint="Krótsza przekątna"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/przekatnaAProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/przekatnaAProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="353dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPrzekatnaAProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="430dp"
                        android:hint="Dłuższa przekątna"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/przekatnaBProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/przekatnaBProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="433dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPrzekatnaBProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="510dp"
                        android:hint="Pole"
                        android:inputType="numberDecimal"
                        android:ems="10"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/poleProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/poleProstokatny"
                        android:text="cm2"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="513dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPoleProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="590dp"
                        android:hint="Obwód"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/obwodProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/obwodProstokatny"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="593dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaObwodProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="670dp"
                        android:hint="Kąt ostry"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/katAProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/katAProstokatny"
                        android:text="@string/stopnie"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="665dp"
                        android:textSize="28dp"
                        android:gravity="left|top"
                        android:id="@+id/jednostkaKatAProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="750dp"
                        android:hint="Kąt rozwarty"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/katBProstokatny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/katBProstokatny"
                        android:text="@string/stopnie"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="745dp"
                        android:textSize="28dp"
                        android:gravity="left|top"
                        android:id="@+id/jednostkaKatBProstokatny"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="830dp"
                        android:hint="Kąt między przekątnymi"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/katCRownoleglobok"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/katCRownoleglobok"
                        android:text="@string/stopnie"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="825dp"
                        android:textSize="28dp"
                        android:gravity="left|top"
                        android:id="@+id/jednostkaKatCRownoleglobok"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="220dp"
                    android:layout_marginTop="1040dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                    <CheckBox
                        android:id="@+id/checkboxPoleProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Pole"/>
                    <CheckBox
                        android:id="@+id/checkboxObwodProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="10dp"
                        android:text="Obwód"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:id="@+id/checkboxGornaPodstawaProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="160dp"
                        android:text="Górna podstawa"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp" />

                    <CheckBox
                        android:id="@+id/checkboxKatAProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Kąt ostry"/>
                    <CheckBox
                        android:id="@+id/checkboxDolnaPodstawaProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="160dp"
                        android:text="Dolna podstawa"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:id="@+id/checkboxKatBProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Kąt rozwarty"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxPrzekatnaAProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="110dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Krótsza przekątna" />
                    <CheckBox
                        android:id="@+id/checkboxPrzekatnaBProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="110dp"
                        android:textColor="@android:color/white"
                        android:layout_marginEnd="10dp"
                        android:textSize="15dp"
                        android:buttonTint="@color/checkbox1"
                        android:text="Dłuższa przekątna"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxWysokoscProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="60dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Wysokosc" />
                    <CheckBox
                        android:id="@+id/checkboxCzwartyBokProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@android:color/white"
                        android:textSize="15dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="60dp"
                        android:buttonTint="@color/checkbox1"
                        android:text="Czwarty bok"
                        android:layout_marginLeft="10dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="1305dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">

                    <TextView
                        android:id="@+id/pierwszaLiniaProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/drugaLiniaProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="55dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/trzeciaLiniaProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/czwartaLiniaProstokatny"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="165dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/wynikProstokatny"
                        android:layout_width="170dp"
                        android:layout_height="73dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="220dp"
                        android:gravity="center"
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/jednostkaWynikProstokatny"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="234dp"
                        android:layout_alignParentRight="true"
                        android:gravity="center_horizontal"
                        android:layout_toRightOf="@id/wynikProstokatny"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />
                </RelativeLayout>
                <Button
                    android:id="@+id/buttonObliczProstokatny"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:background="@drawable/btn_clicked"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="1650dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginBottom="15dp"
                    android:text="Oblicz" />

            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/rellayoutBottomBar"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_navigation_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:menu="@menu/menu_nowe"
                app:itemTextColor="@drawable/item_selectede"
                app:itemIconTint="@drawable/item_selectede"/>
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/navList"
        android:layout_gravity="left|start"
        android:background="@android:color/white"
        android:divider="@color/dividerColor"
        android:dividerHeight="0.5dp"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_width="300dp"
        android:layout_height="match_parent">
    </ExpandableListView>

</androidx.drawerlayout.widget.DrawerLayout>