<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ModelBohra"
    android:id="@+id/drawerModelBohra_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rellayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:id="@+id/rellayoutTopBar"
            android:background="@android:color/black"
            android:layout_alignParentTop="true">
            <com.google.android.material.appbar.AppBarLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.google.android.material.appbar.AppBarLayout>
            <ImageButton
                android:id="@+id/buttonBar"
                android:layout_width="30dp"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:layout_height="30dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_menu_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/rellayoutTopBar"
            android:layout_above="@+id/rellayoutBottomBar"
            android:id="@+id/rellayoutMiddle"
            android:background="@drawable/gradient_list">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rellayoutTopBar"
                android:layout_above="@+id/rellayoutBottomBar">
                <Button
                    android:id="@+id/buttonCzysc"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/btn_clicked"
                    android:text="Wyczyść wszystko" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="500dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginTop="70dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">
                <EditText
                    android:id="@+id/numerOrbity"
                    android:layout_width="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:hint="Numer orbity"
                    android:gravity="center"
                    android:inputType="number"/>
                <EditText
                    android:id="@+id/numerOrbity2"
                    android:layout_width="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="110dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:gravity="center"
                    android:hint="Numer drugiej orbity"
                    android:inputType="number"/>
                <EditText
                    android:id="@+id/energia"
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="70dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="220dp"
                    android:ems="10"
                    android:hint="Ilość wyem. en."
                    android:backgroundTint="@color/checkbox1"
                    android:gravity="center"
                    android:inputType="numberDecimal|numberSigned"/>
                <TextView
                    android:id="@+id/textView38"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="215dp"
                    android:layout_marginStart="180dp"
                    android:text="*10"
                    android:textColor="@android:color/white"
                    android:textSize="35dp"
                    android:layout_marginLeft="180dp" />
                <EditText
                    android:id="@+id/dziesiatkaEnergia"
                    android:layout_width="40dp"
                    android:layout_height="39dp"
                    android:layout_marginTop="202dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:gravity="center"
                    android:inputType="numberSigned"
                    android:text=""
                    android:textSize="15dp"
                    tools:ignore="MissingConstraints"
                    android:layout_toRightOf="@id/textView38"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="220dp"
                    android:textSize="30dp"
                    android:textColor="@android:color/white"
                    android:text="J"
                    android:layout_toRightOf="@id/dziesiatkaEnergia"
                    android:id="@+id/jednostkaEnergia3" />
                <EditText
                    android:id="@+id/dlugoscFali"
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="70dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="320dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:hint="Długość fali"
                    android:gravity="center"
                    android:inputType="numberDecimal" />
                <TextView
                    android:id="@+id/textView35"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="315dp"
                    android:layout_marginStart="180dp"
                    android:text="*10"
                    android:textColor="@android:color/white"
                    android:textSize="35dp"
                    android:layout_marginLeft="180dp" />
                <EditText
                    android:id="@+id/dziesiatkaDlugosc"
                    android:layout_width="40dp"
                    android:layout_height="39dp"
                    android:layout_marginTop="310dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:gravity="center"
                    android:inputType="numberSigned"
                    android:text=""
                    android:textSize="15dp"
                    tools:ignore="MissingConstraints"
                    android:layout_toRightOf="@id/textView35"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="30dp"
                    android:layout_marginTop="330dp"
                    android:layout_toRightOf="@id/dziesiatkaDlugosc"
                    android:id="@+id/jednostkaDlugosc3"
                    android:text="m"
                    android:textColor="@android:color/white"/>
                <EditText
                    android:id="@+id/czestotliwosc"
                    android:layout_width="110dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="70dp"
                    android:layout_marginLeft="70dp"
                    android:layout_marginTop="420dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:hint="Częstotliwość emit. prom."
                    android:gravity="center"
                    android:inputType="numberDecimal"/>
                <TextView
                    android:id="@+id/textView37"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="415dp"
                    android:layout_marginStart="180dp"
                    android:text="*10"
                    android:textColor="@android:color/white"
                    android:textSize="35dp"
                    android:layout_marginLeft="180dp" />
                <EditText
                    android:id="@+id/dziesiatkaCzestotliwosc"
                    android:layout_width="40dp"
                    android:layout_height="39dp"
                    android:layout_marginTop="400dp"
                    android:ems="10"
                    android:backgroundTint="@color/checkbox1"
                    android:gravity="center"
                    android:inputType="numberSigned"
                    android:text=""
                    android:textSize="15dp"
                    tools:ignore="MissingConstraints"
                    android:layout_toRightOf="@id/textView37"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="420dp"
                    android:textSize="30dp"
                    android:textColor="@android:color/white"
                    android:text="Hz"
                    android:layout_toRightOf="@id/dziesiatkaCzestotliwosc" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="600dp">
                <CheckBox
                    android:id="@+id/checkboxNumer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"
                    android:buttonTint="@color/checkbox1"
                    android:text="Numer orbity" />
                <CheckBox
                    android:id="@+id/checkboxEnergia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/checkboxNumer"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:buttonTint="@color/checkbox1"
                    android:text="Ilość wyemitowanej energii" />
                <CheckBox
                    android:id="@+id/checkboxDlugosc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="60dp"
                    android:buttonTint="@color/checkbox1"
                    android:text="Długość fali"/>
                <CheckBox
                    android:id="@+id/checkboxCzestotliwosc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/checkboxDlugosc"
                    android:layout_marginTop="60dp"
                    android:text="Częstotliwość"
                    android:buttonTint="@color/checkbox1" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="740dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">

                    <TextView
                        android:id="@+id/pierwszaLiniaBohra"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/drugaLiniaBohra"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="55dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/trzeciaLiniaBohra"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/czwartaLiniaBohra"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="165dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/wynikEnergia"
                        android:layout_width="190dp"
                        android:layout_height="73dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="220dp"
                        android:gravity="center"
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/dziesiatkaWynikTekst3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="210dp"
                        android:layout_marginLeft="210dp"
                        android:layout_marginTop="230dp"
                        android:text="*10"
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="35dp" />

                    <TextView
                        android:id="@+id/dziesiatkaWynik"
                        android:layout_width="25dp"
                        android:layout_height="39dp"
                        android:layout_marginTop="215dp"
                        android:layout_toRightOf="@id/dziesiatkaWynikTekst3"
                        android:ems="10"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/jednostkaWynik3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="234dp"
                        android:layout_toRightOf="@id/dziesiatkaWynik"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />
                </RelativeLayout>
                <Button
                    android:id="@+id/buttonEnergia"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="1070dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginBottom="15dp"
                    android:text="Oblicz" />

            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/rellayoutBottomBar"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_navigation_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:menu="@menu/menu_nowe"
                app:itemTextColor="@drawable/item_selectede"
                app:itemIconTint="@drawable/item_selectede"/>
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/navList"
        android:layout_gravity="left|start"
        android:background="@android:color/white"
        android:divider="@color/dividerColor"
        android:dividerHeight="0.5dp"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_width="300dp"
        android:layout_height="match_parent">
    </ExpandableListView>
</androidx.drawerlayout.widget.DrawerLayout>