<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Pole_trojkata_rownoramiennego"
    android:id="@+id/drawerRownoramienny_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rellayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@android:color/black"
            android:id="@+id/rellayoutTopBar"
            android:layout_alignParentTop="true">
            <com.google.android.material.appbar.AppBarLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.google.android.material.appbar.AppBarLayout>
            <ImageButton
                android:id="@+id/buttonBar"
                android:layout_width="30dp"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:layout_height="30dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_menu_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/rellayoutTopBar"
            android:layout_above="@+id/rellayoutBottomBar"
            android:background="@drawable/gradient_list"
            android:id="@+id/rellayoutMiddle">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rellayoutTopBar"
                android:layout_above="@+id/rellayoutBottomBar">
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_clicked"
                    android:text="Wyczyść wszystko"
                    android:id="@+id/buttonCzyscRownoramienny"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="935dp"
                    android:layout_marginTop="70dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/textview_rounded_black"
                    android:id="@+id/pierwszyFragmencikRownoramienny">
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:hint="Podstawa"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/podstawaRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaPodstawa"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="36dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="130dp"
                        android:hint="Ramię"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/ramieRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaRamie"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="136dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="230dp"
                        android:hint="Obwód"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:id="@+id/obwodRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaObwod"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="236dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="330dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Pole"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/poleRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm2"
                        android:id="@+id/jednostkaPole"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="336dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="430dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Wysokość"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/wysokoscRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaWysokosc"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="436dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="530dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Promień okr. wpis."
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/wpisanyRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaWpisany"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="536dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="630dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Promień okr. opis."
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/opisanyRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/podstawaRownoramienny"
                        android:textSize="26dp"
                        android:text="cm"
                        android:id="@+id/jednostkaOpisany"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="636dp"/>
                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="730dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Kąt przy wierz. A i B"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/katARownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/katARownoramienny"
                        android:textSize="26dp"
                        android:textColor="@android:color/white"
                        android:text="@string/stopnie"
                        android:gravity="left"
                        android:layout_marginTop="728dp"/>

                    <EditText
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="830dp"
                        android:backgroundTint="@android:color/white"
                        android:textColor="@android:color/white"
                        android:hint="Kąt przy wierz. C"
                        android:gravity="center_horizontal"
                        android:inputType="numberDecimal"
                        android:id="@+id/katCRownoramienny"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@+id/katCRownoramienny"
                        android:textSize="26dp"
                        android:textColor="@android:color/white"
                        android:text="@string/stopnie"
                        android:gravity="left"
                        android:layout_marginTop="828dp"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="210dp"
                    android:layout_below="@+id/pierwszyFragmencikRownoramienny"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginTop="45dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:id="@+id/drugiFragmencikRownoramienny">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Podstawa"
                        android:layout_marginTop="60dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/podstawaWybor"
                        android:textColor="@android:color/white"
                        android:textSize="18dp"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Ramię"
                        android:layout_toRightOf="@id/poleWybor"
                        android:layout_marginTop="10dp"
                        android:layout_centerHorizontal="true"
                        android:textSize="18dp"
                        android:id="@+id/ramieWybor"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Obwód"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="60dp"
                        android:layout_centerHorizontal="true"
                        android:textSize="18dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/obwodWybor"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pole"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:textSize="16dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/poleWybor"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Wysokość"
                        android:layout_marginTop="60dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="8dp"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/wysokoscWybor"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prom. okr. opis."
                        android:layout_marginTop="110dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/opisanyWybor"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Prom. okr. wpis."
                        android:layout_marginTop="110dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:textSize="18dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/wpisanyWybor"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kąt A"
                        android:layout_marginTop="10dp"
                        android:layout_toLeftOf="@id/katCWybor"
                        android:layout_centerHorizontal="true"
                        android:textSize="16dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/katAWybor"/>
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kąt C"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="10dp"
                        android:textSize="16dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:buttonTint="@color/checkbox1"
                        android:id="@+id/katCWybor"
                        android:layout_marginRight="10dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:background="@drawable/textview_rounded_black"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginTop="45dp"
                    android:id="@+id/trzeciFragmencikRownoramienny"
                    android:layout_below="@id/drugiFragmencikRownoramienny"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:id="@+id/pierwszaLiniaRownoramienny"
                        android:textColor="@android:color/white"
                        android:layout_centerHorizontal="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:layout_marginTop="55dp"
                        android:id="@+id/drugaLiniaRownoramienny"
                        android:textColor="@android:color/white"
                        android:layout_centerHorizontal="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:layout_marginTop="110dp"
                        android:id="@+id/trzeciaLiniaRownoramienny"
                        android:textColor="@android:color/white"
                        android:layout_centerHorizontal="true"
                        android:text=""/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20dp"
                        android:layout_marginTop="165dp"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/czwartaLiniaRownoramienny"
                        android:textColor="@android:color/white"
                        android:text=""/>
                    <TextView
                        android:layout_width="180dp"
                        android:layout_height="80dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="220dp"
                        android:textSize="34dp"
                        android:text=""
                        android:gravity="center"
                        android:id="@+id/wynikRownoramienny"
                        android:textColor="@android:color/white"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/wynikRownoramienny"
                        android:textSize="31dp"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:text=""
                        android:layout_marginTop="240dp"
                        android:id="@+id/jednostkaWynik"/>


                </RelativeLayout>
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:id="@+id/buttonRownoramienny"
                    android:text="Oblicz"
                    android:layout_below="@id/trzeciFragmencikRownoramienny"
                    android:layout_marginTop="45dp"
                    android:layout_marginBottom="15dp"/>
            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/rellayoutBottomBar"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_navigation_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:menu="@menu/menu_nowe"
                app:itemTextColor="@drawable/item_selectede"
                app:itemIconTint="@drawable/item_selectede"/>
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/navList"
        android:layout_gravity="left|start"
        android:background="@android:color/white"
        android:divider="@color/dividerColor"
        android:dividerHeight="0.5dp"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_width="300dp"
        android:layout_height="match_parent">
    </ExpandableListView>
</androidx.drawerlayout.widget.DrawerLayout>