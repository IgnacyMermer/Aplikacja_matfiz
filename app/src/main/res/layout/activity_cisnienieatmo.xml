<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CisnienieAtm"
    android:id="@+id/drawerCisnienieAtm_layout"
    tools:ignore="MissingDefaultResource">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rellayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:id="@+id/rellayoutTopBar"
            android:background="@android:color/black"
            android:layout_alignParentTop="true">
            <com.google.android.material.appbar.AppBarLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.google.android.material.appbar.AppBarLayout>
            <ImageButton
                android:id="@+id/buttonBar"
                android:layout_width="30dp"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:layout_height="30dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_menu_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rellayoutTopBar"
            android:layout_above="@id/rellayoutBottomBar"
            android:background="@drawable/gradient_list"
            android:id="@+id/rellayoutMiddle">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rellayoutTopBar"
                android:layout_above="@+id/rellayoutBottomBar">
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:text="Wyczyść wszystko"
                    android:id="@+id/buttonCzyscCisnienieAtm"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="365dp"
                    android:layout_marginTop="70dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:hint="Ciśnienie"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/cisnienieCisnienieAtm"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:hint="Powierzchnia"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/powierzchniaCisnienieAtm"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/cisnienieCisnienieAtm"
                        android:text="Pa"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="33dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaCisnienieCisnienieAtm"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/powierzchniaCisnienieAtm"
                        android:text="m2"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="113dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPowierzchniaCisnienieAtm"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="190dp"
                        android:hint="Siła parcia"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/silaCisnienieAtm"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/silaCisnienieAtm"
                        android:text="N"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="193dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkasilaCisnienieAtm"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="270dp"
                        android:hint="Masa"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/masaCisnienieAtm"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/masaCisnienieAtm"
                        android:text="kg"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="273dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaMasaCisnienieAtm"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="105dp"
                    android:layout_marginTop="480dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                    <CheckBox
                        android:id="@+id/checkboxCisnienieCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Ciśnienie"/>
                    <CheckBox
                        android:id="@+id/checkboxSilaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Siła parcia"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp" />

                    <CheckBox
                        android:id="@+id/checkboxPowierzchniaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Powierzchnia"/>
                    <CheckBox
                        android:id="@+id/checkboxMasaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Masa"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="630dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                    <TextView
                        android:id="@+id/pierwszaLiniaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />
                    <TextView
                        android:id="@+id/drugaLiniaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="55dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />
                    <TextView
                        android:id="@+id/trzeciaLiniaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/czwartaLiniaCisnienieAtm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="165dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/wynikCisnienieAtm"
                        android:layout_width="170dp"
                        android:layout_height="73dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="220dp"
                        android:gravity="center"
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/jednostkaWynikCisnienieAtm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="234dp"
                        android:layout_alignParentRight="true"
                        android:gravity="center_horizontal"
                        android:layout_toRightOf="@id/wynikCisnienieAtm"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />
                </RelativeLayout>
                <Button
                    android:id="@+id/buttonObliczCisnienieAtm"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="975dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginBottom="15dp"
                    android:text="Oblicz" />

            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/rellayoutBottomBar"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_navigation_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:menu="@menu/menu_nowe"
                app:itemTextColor="@drawable/item_selectede"
                app:itemIconTint="@drawable/item_selectede"/>
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/navList"
        android:layout_gravity="left|start"
        android:background="@android:color/white"
        android:divider="@color/dividerColor"
        android:dividerHeight="0.5dp"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_width="300dp"
        android:layout_height="match_parent">
    </ExpandableListView>

</androidx.drawerlayout.widget.DrawerLayout>