<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".RuchPoOkregu"
    android:id="@+id/drawerRuchPoOkregu_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rellayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:id="@+id/rellayoutTopBar"
            android:background="@android:color/black"
            android:layout_alignParentTop="true">
            <com.google.android.material.appbar.AppBarLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.google.android.material.appbar.AppBarLayout>
            <ImageButton
                android:id="@+id/buttonBar"
                android:layout_width="30dp"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:layout_height="30dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_menu_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/rellayoutMiddle"
            android:layout_below="@+id/rellayoutTopBar"
            android:layout_above="@+id/rellayoutBottomBar"
            android:background="@drawable/gradient_list">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rellayoutTopBar"
                android:layout_above="@+id/rellayoutBottomBar">
                <Button
                    android:id="@+id/czysciciel"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/btn_clicked"
                    android:text="Wyczyść wszystko" />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="750dp"
                    android:layout_marginTop="70dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp">
                <EditText
                    android:id="@+id/promien"
                    android:layout_width="136dp"
                    android:layout_height="49dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="30dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Promień okręgu"
                    android:inputType="numberDecimal"/>
                <TextView
                    android:id="@+id/jednostkaPromien"
                    android:layout_width="wrap_content"
                    android:layout_height="49dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="35dp"
                    android:gravity="center"
                    android:textColor="@android:color/white"
                    android:text="m"
                    android:textSize="26dp"
                    tools:ignore="MissingConstraints" />
                <EditText
                    android:id="@+id/obwod"
                    android:layout_width="136dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="16dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Obwód okręgu"
                    android:inputType="numberDecimal"
                    tools:ignore="MissingConstraints"
                    android:layout_marginLeft="200dp"
                    android:layout_marginRight="16dp" />
                <EditText
                    android:id="@+id/czestotliwosc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="110dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Częstotliwość"
                    android:inputType="numberDecimal"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaCzestotliwosc"
                    android:textSize="26dp"
                    android:gravity="center_horizontal"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="115dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/czestotliwosc"
                    android:text="Hz"/>
                <EditText
                    android:id="@+id/okres"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="190dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Okres ruchu"
                    android:inputType="numberDecimal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaOkres"
                    android:textSize="26dp"
                    android:gravity="center_horizontal"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="195dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/okres"
                    android:text="s"/>
                <EditText
                    android:id="@+id/czas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="270dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Czas"
                    android:inputType="numberDecimal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaCzas"
                    android:textSize="26dp"
                    android:gravity="center_horizontal"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="275dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/czas"
                    android:text="s"/>
                <EditText
                    android:id="@+id/predkosc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="350dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Prędkość"
                    android:inputType="numberDecimal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaPredkosc"
                    android:textSize="26dp"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="355dp"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/predkosc"
                    android:text="m/s"/>
                <EditText
                    android:id="@+id/przyszpieszenie2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="430dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:gravity="center"
                    android:hint="Przyszpieszenie"
                    android:inputType="numberDecimal"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaPrzyszpieszenie"
                    android:textSize="26dp"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="435dp"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/przyszpieszenie2"
                    android:text="m/s2"/>
                <EditText
                    android:id="@+id/sila2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="510dp"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Siła dośrodkowa"
                    android:inputType="numberDecimal"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaSila"
                    android:textSize="26dp"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="515dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/sila2"
                    android:gravity="center_horizontal"
                    android:text="N"/>
                <EditText
                    android:id="@+id/masa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="590dp"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:ems="10"
                    android:hint="Masa"
                    android:gravity="center"
                    android:inputType="numberDecimal"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaMasa"
                    android:textSize="26dp"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="595dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_alignParentRight="true"
                    android:layout_toRightOf="@id/masa"
                    android:gravity="center_horizontal"
                    android:text="kg"/>
                <EditText
                    android:id="@+id/liczbaOkrazen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="670dp"
                    android:ems="10"
                    android:textColor="@android:color/white"
                    android:backgroundTint="@android:color/white"
                    android:hint="Liczba okrążeń"
                    android:gravity="center"
                    android:inputType="numberDecimal" />
            </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:layout_marginTop="865dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                <CheckBox
                    android:id="@+id/promienWybor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/white"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:buttonTint="@color/checkbox1"
                    android:text="Promien" />
                <CheckBox
                    android:id="@+id/czestotliwoscWybor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:buttonTint="@color/checkbox1"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="10dp"
                    android:text="Czestotliwosc" />
                <CheckBox
                    android:id="@+id/okresWybor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:buttonTint="@color/checkbox1"
                    android:layout_marginTop="60dp"
                    android:textColor="@android:color/white"
                    android:layout_marginLeft="10dp"
                    android:text="Masa" />
                <CheckBox
                    android:id="@+id/czasWybor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:buttonTint="@color/checkbox1"
                    android:layout_marginTop="110dp"
                    android:text="Czas"
                    android:textColor="@android:color/white"
                    android:layout_marginLeft="15dp" />
                <CheckBox
                    android:id="@+id/predkoscWybor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:textColor="@android:color/white"
                    android:buttonTint="@color/checkbox1"
                    android:text="Prędkość" />
                <CheckBox
                    android:id="@+id/checkboxSila"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="60dp"
                    android:buttonTint="@color/checkbox1"
                    android:textColor="@android:color/white"
                    android:text="Siła" />
                <CheckBox
                    android:id="@+id/checkBoxPrzyszpieszenie"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:buttonTint="@color/checkbox1"
                    android:layout_marginTop="60dp"
                    android:textColor="@android:color/white"
                    android:text="Przyszpieszenie"/>
                <CheckBox
                    android:id="@+id/checkBoxOkress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/white"
                    android:layout_alignParentRight="true"
                    android:buttonTint="@color/checkbox1"
                    android:layout_marginTop="110dp"
                    android:text="Okres"
                    android:layout_marginRight="10dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginTop="1070dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp">
                    <TextView
                        android:id="@+id/pierwszaLiniaPoOkregu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/drugaLiniaPoOkregu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="55dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/trzeciaLiniaPoOkregu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/czwartaLiniaPoOkregu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="165dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />
                <TextView
                    android:id="@+id/wynik"
                    android:layout_width="150dp"
                    android:layout_height="140dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="220dp"
                    android:gravity="center"
                    android:textIsSelectable="true"
                    android:textColor="@android:color/white"
                    android:textSize="27dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/jednostkaWynik"
                    android:textSize="26dp"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="243dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_toRightOf="@id/wynik"
                    android:text=""/>
                </RelativeLayout>
                <Button
                    android:id="@+id/button"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="1415dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_marginLeft="15dp"
                    android:layout_marginBottom="15dp"
                    android:layout_marginRight="15dp"
                    android:text="Oblicz" />

            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/rellayoutBottomBar"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_navigation_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:menu="@menu/menu_nowe"
                app:itemTextColor="@drawable/item_selectede"
                app:itemIconTint="@drawable/item_selectede"/>
        </RelativeLayout>


    </RelativeLayout>
    <LinearLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/navList"
        android:layout_gravity="left|start"
        android:background="@android:color/white"
        android:divider="@color/dividerColor"
        android:dividerHeight="0.5dp"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_width="300dp"
        android:layout_height="match_parent">
    </ExpandableListView>
</androidx.drawerlayout.widget.DrawerLayout>