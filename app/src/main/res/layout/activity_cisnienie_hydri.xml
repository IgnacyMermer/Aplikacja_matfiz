<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CisnienieHydri"
    android:id="@+id/drawerCisnienieHydro_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rellayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:id="@+id/rellayoutTopBar"
            android:background="@android:color/black"
            android:layout_alignParentTop="true">
            <com.google.android.material.appbar.AppBarLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </com.google.android.material.appbar.AppBarLayout>
            <ImageButton
                android:id="@+id/buttonBar"
                android:layout_width="30dp"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:layout_height="30dp"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_menu_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </RelativeLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rellayoutTopBar"
            android:layout_above="@id/rellayoutBottomBar"
            android:background="@drawable/gradient_list"
            android:id="@+id/rellayoutMiddle">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rellayoutTopBar"
                android:layout_above="@+id/rellayoutBottomBar">
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:layout_marginTop="4dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:text="Wyczyść wszystko"
                    android:id="@+id/buttonCzyscCisnienieHydro"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="825dp"
                    android:layout_marginTop="70dp"
                    android:background="@drawable/textview_rounded_black"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp">
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:hint="Ciśnienie"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/cisnienieCisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:hint="Wysokość"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/wysokoscCisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/cisnienieCisnienieHydro"
                        android:text="Pa"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="33dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaCisnienieCisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/wysokoscCisnienieHydro"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="113dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaWysokoscCisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="190dp"
                        android:hint="Przesunięcie tłoka"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/przesuniecieCisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/przesuniecieCisnienieHydro"
                        android:text="cm"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="193dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPrzesuniecieCisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="270dp"
                        android:hint="Powierzchnia 1 (mały tłok)"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/powierzchnia1CisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/powierzchnia1CisnienieHydro"
                        android:text="cm2"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="273dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPowierzchnia1CisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="350dp"
                        android:hint="Powierzchnia 2 (duży tłok)"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/powierzchnia2CisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/powierzchnia2CisnienieHydro"
                        android:text="cm2"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="353dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaPowierzchnia2CisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="430dp"
                        android:hint="Siła nacisku 1 (mały tłok)"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/silaNac1CisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/silaNac1CisnienieHydro"
                        android:text="N"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="433dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaSilaNac1CisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="510dp"
                        android:hint="Siła nacisku 2 (duży tłok)"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/silaNac2CisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/powierzchnia2CisnienieHydro"
                        android:text="N"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="513dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaSilaNac2CisnienieHydro"/>
                    <EditText
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="590dp"
                        android:hint="Gęstość cieczy"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/gestoscCisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/gestoscCisnienieHydro"
                        android:text="kg/m3"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="593dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaGestoscCisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="670dp"
                        android:hint="Masa cieczy"
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/masaCisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/masaCisnienieHydro"
                        android:text="kg"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="673dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaMasaCisnienieHydro"/>
                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="750dp"
                        android:hint="Ciśnienie atm."
                        android:ems="10"
                        android:inputType="numberDecimal"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/white"
                        android:backgroundTint="@android:color/white"
                        android:id="@+id/cisnienieAtmCisnienieHydro"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@id/cisnienieAtmCisnienieHydro"
                        android:text="Pa"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="753dp"
                        android:textSize="28dp"
                        android:gravity="center_horizontal"
                        android:id="@+id/jednostkaCisnienieAtmCisnienieHydro"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="165dp"
                    android:layout_marginTop="950dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                    <CheckBox
                        android:id="@+id/checkboxCisnienieCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Ciśnienie"/>
                    <CheckBox
                        android:id="@+id/checkboxSilaCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Siła"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxPowierzchniaCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Powierzchnia"/>
                    <CheckBox
                        android:id="@+id/checkboxWysokoscCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="60dp"
                        android:text="Wysokość"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxPrzesuniecieCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="110dp"
                        android:text="Przesunięcie"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:layout_marginRight="10dp" />
                    <CheckBox
                        android:id="@+id/checkboxGestoscCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="110dp"
                        android:textSize="15dp"
                        android:textColor="@android:color/white"
                        android:buttonTint="@color/checkbox1"
                        android:text="Gęstość"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="1160dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/textview_rounded_black">
                    <TextView
                        android:id="@+id/pierwszaLiniaCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />
                    <TextView
                        android:id="@+id/drugaLiniaCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="55dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />
                    <TextView
                        android:id="@+id/trzeciaLiniaCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="110dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/czwartaLiniaCisnienieHydro"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="165dp"
                        android:text=""
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/wynikCisnienieHydro"
                        android:layout_width="170dp"
                        android:layout_height="73dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="220dp"
                        android:gravity="center"
                        android:textIsSelectable="true"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/jednostkaWynikCisnienieHydro"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="234dp"
                        android:layout_alignParentRight="true"
                        android:gravity="center_horizontal"
                        android:layout_toRightOf="@id/wynikCisnienieHydro"
                        android:textColor="@android:color/white"
                        android:textSize="30dp" />
                </RelativeLayout>
                <Button
                    android:id="@+id/buttonObliczCisnienieHydro"
                    android:layout_width="match_parent"
                    android:layout_height="62dp"
                    android:background="@drawable/btn_clicked"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="1505dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginBottom="15dp"
                    android:text="Oblicz" />

            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/rellayoutBottomBar"
            android:layout_alignParentBottom="true">
            <com.google.android.material.bottomnavigation.BottomNavigationView
                android:id="@+id/bottom_navigation_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:menu="@menu/menu_nowe"
                app:itemTextColor="@drawable/item_selectede"
                app:itemIconTint="@drawable/item_selectede"/>
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:id="@+id/container2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/navList"
        android:layout_gravity="left|start"
        android:background="@android:color/white"
        android:divider="@color/dividerColor"
        android:dividerHeight="0.5dp"
        android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
        android:layout_width="300dp"
        android:layout_height="match_parent">
    </ExpandableListView>

</androidx.drawerlayout.widget.DrawerLayout>